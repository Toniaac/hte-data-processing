@article{Lin2025,
author = {Dian-Zhao Lin  and Kai-Jui Pan  and Yuyin Li  and Charles B. null  and Lingyu Zhang  and Krish N. Jayarapu  and Tianchen Li  and Jasmine Vy Tran  and William A. Goddard  and Zhengtang Luo  and Yayuan Liu },
